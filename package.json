{
  "name": "@luxcium/boxed-list",
  "version": "0.0.0-PREALPHA-v0.0x",
  "license": "MIT",
  "private": false,
  "description": "",
  "main": "./out/index.js",
  "types": "./out/typings/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "isLibrary": true,
  "scripts": {
    "SPLIT:00": "#---------------------------------------------------",
    "prepare": "echo preparing;npm run --foreground-scripts build:quick; echo prepared",
    "base:prerelease": "./scripts/prerelease.sh",
    "base:release": "./scripts/release.sh",
    "base:check": "tsc --noEmit",
    "base:prestart": "rm -fr ./out; tsc -b src/tsconfig.json",
    "base:version": "ts-node -vv;tsc -v",
    "prebuild:task": "rm -fr ./out",
    "build:task": "mkdir -p \"./out/performance\" && tsc -b --extendedDiagnostics 'src/tsconfig.json' --generateCpuProfile \"./out/performance\" || exit 1",
    "postbuild:task": "# NONE",
    "prebuild": "npm ci; npm run fix && npm run test || exit 7",
    "build": "mkdir -p \"./out/performance\" && tsc -b 'src/tsconfig.json' --generateCpuProfile \"./out/performance\" || exit 1",
    "build:quick": "mkdir -p \"./out/performance\" && tsc -b 'src/tsconfig.json' --generateCpuProfile \"./out/performance\" || exit 1",
    "precibuild": "npm run build || exit 2",
    "SPLIT:01": "#---------------------------------------------------",
    "cibuild": "tsc --build \"tsconfig.src-build.json\" || exit 1",
    "postcibuild": " ( [ \"$(npm audit)\" = 'found 0 vulnerabilities' ] && echo 'found 0 vulnerabilities' || exit 1) &&  [ \"${ZE160DDEEx}\" = \"1050D673\" ] && (coveralls < coverage/lcov.info); exit 0",
    "coverage": "jest --verbose false --silent true --coverage || exit 1",
    "lint": "npm run pretty && npx eslint \"./src/**/*.*\" --fix-dry-run || exit 1",
    "lint:fix": "npx eslint \"./src/**/*.{j,t}s\" --fix || exit 1",
    "lint:json": "eslint \"**/*.{jsonc,json}\" --fix-dry-run || exit 1",
    "lint:fix:json": "eslint \"**/*.{jsonc,json}\" --fix || exit 1",
    "pretty": "prettier --config \"./.prettierrc\" --loglevel log -c \"./src/**/*.*\"|| exit 1",
    "pretty:all": "prettier --config \"./.prettierrc\" --loglevel log -c \"**/*.*\" || exit 1",
    "pretty:fix": "npx prettier --config \"./.prettierrc\" --loglevel log -c -w \"./src/**/*.*\" || exit 1",
    "pretty:fix:json": "npx prettier --config \"./.prettierrc\" --loglevel log -c -w \"**/*.{jsonc,json}\" || exit 1",
    "pretty:fix:all": "prettier --config \"./.prettierrc\" --loglevel log -c -w \"**/*.*\" || exit 1",
    "prefix": "#npx eslint --print-config src/index.ts | npx eslint-config-prettier-check || exit 1",
    "fix": "npm run lint:fix && npm run pretty:fix || exit 4",
    "fix:json": "npm run lint:fix:json && npm run pretty:fix:json || exit 4",
    "fix:and:json": "npm run fix:json && npm run fix || exit 8",
    "test": "jest --verbose true || exit 1",
    "precheck": "npm run fix || exit 5",
    "check": "npm run test && tsc --noEmit --listFiles ||exit 3",
    "postcheck": "npm run coverage || exit 2",
    "tsc:version": "tsc --version",
    "serve-coverage": "scripts/server-coverage.sh",
    "serve-docs": "scripts/server-docs.sh",
    "make:docs": "npx typedoc --sort static-first --disableSources --excludeProtected --excludePrivate --tsconfig ./src/tsconfig.json ./src/index.ts",
    "SPLIT:XX": "#---------------------------------------------------"
  },
  "keywords": [],
  "author": "Benjamin Vincent (Luxcium)",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@types/chai": "^4.3.0",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16",
    "@types/prettier": "^2.4.4",
    "@types/sharp": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "am": "^1.1.0",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.6",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^41.0.1",
    "fantasy-laws": "^2.0.1",
    "got": "^12.0.2",
    "jest": "^27.5.1",
    "jsverify": "^0.8.4",
    "node-notifier": "^10.0.1",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "sanctuary-show": "^3.0.0",
    "sanctuary-type-classes": "^13.0.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "typedoc": "^0.22.13"
  }
}
